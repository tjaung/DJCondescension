{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "esnext",
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "target": "es6",
    "lib": ["dom", "es2015", "esnext"],
    "types": ["node","three"],
    "jsx": "react-jsx"
  },
  "include": ["src/**/*", "src/api/spotifyAPI.ts"],
  "exclude": ["node_modules"]
}
